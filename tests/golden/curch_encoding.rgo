@/str
@/int
@/add
@/printf
@/exit

zero: (f: (int, (int)), x: int, ok: (int)){
    ok(x)
}
one: (f: (int, (int)), x: int, ok: (int)){
    f(x, ok)
}
two: (f: (int, (int)), x: int, ok: (int)){
    f(x, (y: int){
        f(y, ok)
    })
}
three: (f: (int, (int)), x: int, ok: (int)){
    f(x, (y: int){
        f(y, (z: int){
            f(z, ok)
        })
    })
}
three((x: int, ok:(int)){
    add(x, 10, ok)
}, 0, (x: int){
    printf("result: %d\n", x, exit(0))
})
