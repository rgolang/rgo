str: @/str
int: @/int
exit: @/exit
printf: @/printf
add: @/add
eqi: @/eqi

foo: (x:int, y:int, ok:(int)){
    add(x, y, ok)
}
run: (ok:()){
    f: foo(1)
    a: f(2)
    b: f(3)
    k: (x:int){
        printf("number: %d", x, ok) 
    }
    eqi(1, 1, {
        a(k)
    }, {
        b(k)
    })
}
run(exit(0))
